<%include header.ejs%><h1><%=title%></h1><form action="" method="post">    <%=title%>    <%=genre%>    <div class="form-group">        <label for="name">Genre:</label>        <input type="text" id="name" class="form-control" name="name" value=<%=(genre=="undefined"?"":genre.name)%>>        <button class="btn btn-primary" type="submit">            Submit        </button>    </div>    <% if(errors){%>    <ul>     <% errors.forEach(function (item) {%>       <li><%=item.msg%></li>     <%})%>    </ul>    <%}%>    </form><%include footer.ejs%>