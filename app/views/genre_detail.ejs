<% include header.ejs %>

<h1> Genre: <%= genre.name %></h1>

<div style="margin-left: 20px ;margin-top: 20px">
    <h4>Books</h4>
    <dl>
        <% genre_books.forEach(function(item){ %>
        <dt>
            <a href="<%= item.url %>"><%= item.title %></a>
        <dd>
            <%= item.summary %>
        </dd>
        </dt>
        <% }) %>
    </dl>
</div>

<% include footer.ejs %>