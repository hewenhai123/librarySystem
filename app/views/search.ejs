<% include header.ejs%>
<div class="container" style="margin-top:100px">
    <form action="" method="post">
        <fieldset >
            <div class="form-group">
                <label for="">房屋结构</label>
                <select id="" class="form-control">
                    <%if(data.hstructures){%>
                        <% data.hstructures.forEach(function(item){%>
                            <option value=<%= item.stname%>><%= item.stname%></option>
                        <%})%>
                    <%}%>
                </select>
            </div>
            <div class="form-group">
                <label for="">房屋区域</label>
                <select id="" class="form-control">
                    <%if(data.hcountys){%>
                    <% data.hcountys.forEach(function(item){%>
                    <option value=<%= item.ctname%>><%= item.ctname%></option>
                    <%})%>
                    <%}%>
                </select>
            </div>
            <div class="form-group">
                <label for="">房屋类型</label>
                <select id="" class="form-control">
                    <%if(data.preordains){%>
                    <% data.preordains.forEach(function(item){%>
                    <option value=<%= item.tyname%>> <%=item.tyname%></option>
                    <%})%>
                    <%}%>
                </select>
            </div>

            <button type="submit" class="btn btn-primary">查询</button>
        </fieldset>
    </form>
</div>


<% include footer.ejs %>